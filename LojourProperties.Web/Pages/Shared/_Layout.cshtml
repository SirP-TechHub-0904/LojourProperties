    <script src="~/smart/js/modernizr.js"></script>

    <style>
        .page-header-area-2 {
            padding: 30px 0;
            text-align: left;
            position: relative;
            background-image: url('../../img/hd.jpg');
            background-position: center;
            background-repeat: no-repeat;
            background-size: cover;
        }

        .small-breadcrumb .breadcrumb-link ul li::after {
            color: #fff;
            content: "\f105";
            font-family: "FontAwesome";
            font-size: 13px;
            font-style: normal;
            font-weight: 500;
            position: absolute;
            right: 0;
            text-decoration: inherit;
            top: 10px;
            margin-right: 5px;
        }

    </style>
