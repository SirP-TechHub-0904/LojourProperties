        <!-- Navigation Menu -->
        <div class="clearfix"></div>
        <!-- menu start -->
        <nav id="menu-1" class="mega-menu">
            <!-- menu list items container -->
            <section class="menu-list-items">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 col-md-12">
                            <!-- menu logo -->
                            <ul class="menu-logo">
                                <li>
                                    <a href="/"><img src="~/img/lojour-logo-green.png" alt="LOJOUR PROPERTIES" style="margin-top:8px;"> </a>
                                </li>
                            </ul>
                            <!-- menu links -->
                            <ul class="menu-links">
                                <!-- active class -->
                                <li>
                                    <a asp-page="/Property" asp-route-category="Rent">Rent</a>
                                </li>
                                <li>
                                    <a asp-page="/Property" asp-route-category="Lease">Lease</a>
                                </li>
                                <li>
                                    <a asp-page="/Property" asp-route-category="Sale">Sale</a>
                                </li>

                                <li>
                                    <a asp-page="/Property" asp-route-category="SmartHomes">SmartHomes</a>
                                </li>
                                <li>
                                    <a asp-page="/Property" asp-route-category="Serviced Apartment">
                                        Serviced Apartment
                                    </a>
                                </li>
                                <li>
                                    <a asp-page="/Property" asp-route-find="true">
                                        Find a Home
                                    </a>
                                </li>
                            </ul>
                            <ul class="menu-search-bar">
                                <li>
                                    <a asp-page="/Request" class="btn btn-theme">Special Request</a>

                                    @if (!User.Identity.IsAuthenticated)
                                    {
                                        <a asp-page="/Account/Login" asp-area="Identity" class="btn btn-theme">Log in</a>

                                    }
                                    else if (User.Identity.IsAuthenticated)
                                    {
                                        <a asp-page="/V1/Dashboard/Index" asp-area="Admin" class="btn btn-theme"><i class="fa fa-dashboard"></i></a>


                                        <a href="javascript:document.getElementById('logoutForm').submit()" class="btn btn-theme"><i class="fa fa-sign-out"></i></a>


                                    }
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </section>
        </nav>
        <!-- menu end -->
    </div>
    <form asp-area="Identity" id="logoutForm" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Page("/Account/Login", new { area = "Identity" })" method="post">
    </form>
    <div class="clearfix"></div>


    @RenderBody()
    <!-- Back To Top -->
    <a href="#0" class="cd-top">Top</a>
