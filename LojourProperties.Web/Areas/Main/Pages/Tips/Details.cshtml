@page
@model LojourProperties.Web.Areas.Main.Pages.Tips.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">DETAILS </h3>


    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.SafetyTip.Message)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.SafetyTip.Message)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.SafetyTip.SortOrder)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.SafetyTip.SortOrder)
                    </dd>

                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.SafetyTip.Show)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.SafetyTip.Show)
                    </dd>
                </dl>
            </div>
            <div>
                <a asp-page="./Edit" asp-route-id="@Model.SafetyTip.Id">Edit</a> |
                <a asp-page="./Index">Back to List</a>
            </div>


            <!-- /.row -->
        </div>
        <!-- /.card-body -->
        <div class="card-footer">
        </div>
    </div>
